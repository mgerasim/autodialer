<p id="notice"><%= notice %></p>

<h1>Configs</h1>

<table>
  <thead>
    <tr>
      <th>Password encrypted</th>
      <th>Is outgoing deleted</th>
      <th>Is outgoing table showed</th>
      <th>Is google integrated</th>
      <th>Is attempt supported</th>
      <th>Is answer supported</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @configs.each do |config| %>
      <tr>
        <td><%= config.password_encrypted %></td>
        <td><%= config.is_outgoing_deleted %></td>
        <td><%= config.is_outgoing_table_showed %></td>
        <td><%= config.is_google_integrated %></td>
        <td><%= config.is_attempt_supported %></td>
        <td><%= config.is_answer_supported %></td>
        <td><%= link_to 'Show', config %></td>
        <td><%= link_to 'Edit', edit_config_path(config) %></td>
        <td><%= link_to 'Destroy', config, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Config', new_config_path %>
